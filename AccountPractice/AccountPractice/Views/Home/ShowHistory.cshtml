@model IEnumerable<AccountPractice.Models.MoneyViewModel>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ShowHistory</title>
</head>
<body>
    <div class="row">
        <div class="col-md-12">
            <table class="table table-bordered table-hover">
                @{
                    var i = 1;
                }
                <tr>
                    <th>#</th>
                    <th>類別</th>
                    <th>日期</th>
                    <th>金額</th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@i</td>
                        <td>@item.memo</td>
                        <td>@item.date</td>
                        <td>@item.price</td>
                    </tr>
                    i++;
                }


            </table>
        </div>
    </div>

</body>
</html>
